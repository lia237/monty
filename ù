#include "monty.h"

void print_all_stack(stack_t **stack_top, unsigned int line_number)
{
    stack_t *current;
    (void) line_number;

    current = *stack_top;
    if (current == NULL)
        return;

    while (current)
    {
        printf("%d\n", current->n);
        current = current->next;
    }
}

